<%include partials/header%>
<title>Google Maps Plugin</title>
<link rel="stylesheet" type="text/css" href="/CSS/locationsInput.css">
<link rel="stylesheet" type="text/css" href="/CSS/map.css">
</head>
<!--<body oncontextmenu="return false" onkeydown="return false;" onmousedown="return false;">-->
	<body>
		<div class="container noSpace">
			<div class="row editorRow">
				<div class="col-lg-4 editor text-center probootstrap-animate fadeInLeft probootstrap-animated">
					<div class="choose row padding-top-20">
						<div class="col-lg-6">
							<p id="contentBtn" class="selected">Content</p>
						</div>
						<div class="col-lg-6 text-center">
							<p id="designBtn">Design</p>
						</div>
					</div>
					<!--location slide-->
					<div id="location" class="row text-left">
						<div class="col-lg-12 padding-left-50 padding-top-20">
							<p>Locations</p>
						</div>
						<div class="scrollDiv">

						</div>
						<div class="col-lg-12 padding-top-20 padding-left-50 padding-right-50 addLocation33">
							<a class="btn btn-primary  btn-block addLocationBtn" role="button"><i class="fas fa-plus padding-right-20"></i>Add Location</a>
						</div>
						<div class="col-lg-6 padding-top-20" style="padding-left: 33px;">
							<a href="/" class="btn btn-primary  btn-block" role="button">Back</a>
						</div>
						<div class="col-lg-6 padding-top-20" style="padding-right: 33px;">
							<a id="toDesign" class="btn btn-primary  btn-block" role="button">Next</a>
						</div>
					</div>
					<!--end location slide-->

					<!--start editlocation slide-->
					<div id="editLocation" class="row text-left">
						<div class="col-lg-12 padding-left-50 padding-top-20">
							<p>Edit Location</p>
						</div>
						<div class="col-lg-12" style="height: 380px;">
							<form id="my_form">
								<div class="col-lg-12 padding-top-20">
									<div class="col-lg-4 padding-left-20">
										<p class="padding-top-10">Title:</p>
									</div>
									<div class="col-lg-8">
										<input class="grey" type="text" name="title" placeholder="Company's Name">
									</div>
								</div>

								<div class="col-lg-12 padding-top-20">
									<div class="col-lg-4 padding-left-20">
										<p class="padding-top-10">Address:</p>
									</div>
									<div class="col-lg-8">
										<input class="grey" type="text" name="address" placeholder="Street Address, City, State, Zip Code"  required>
									</div>
								</div>

								<div class="col-lg-12 padding-top-20">
									<div class="col-lg-4 padding-left-20">
										<p class="padding-top-10">Link:</p>
									</div>
									<div class="col-lg-8">
										<input class="grey" type="text" name="link" placeholder="Company's Link">
									</div>
								</div>

								<div class="col-lg-12 padding-top-20">
									<div class="col-lg-4 padding-left-20">
										<p class="padding-top-10">Phone Number:</p>
									</div>
									<div class="col-lg-8">
										<input class="grey" type="text" name="phoneNumber" placeholder="Company's Phone Number">
									</div>
								</div>
								<input style="display: none;" class="grey" type="text" name="placeId" placeholder="Company's Phone Number">

							</div>

							<div class="col-lg-12 padding-top-20 padding-left-50 padding-right-50 addLocation33">
								<a id="submitLocation" class="btn btn-primary  btn-block addLocationBtn" role="button"><i class="fas fa-plus padding-right-20"></i>Save Changes</a>
							</div>
						</form>

						<div class="col-lg-6 padding-top-20" style="padding-left: 33px;">
							<a id="backToLocation" class="btn btn-primary  btn-block" role="button">Back</a>
						</div>
						<div class="col-lg-6 padding-top-20" style="padding-right: 33px;">
							<a disabled class="btn btn-primary  btn-block" role="button">Next</a>
						</div>


					</div>
					<!--end editlocation slide-->

					<!--start design slide-->
					<div id="design" class="row text-left">
						<div class="col-lg-12 padding-left-50 padding-top-20">
							<p>Design</p>
						</div>
						<!--<div class="col-lg-12 padding-left-30 padding-right-30 btnContainer padding-top-20" id="0">-->
						<div style="height: 438px;padding-left: 50px;padding-right: 50px;">
							<div class="col-lg-12 btnDefault text-center default">
								<div class="col-lg-1">
									<i class="glyphicon glyphicon-adjust"></i>
								</div>
								<div class="col-lg-6 ">
									<p>Default</p>
								</div>
								<div class="col-lg-1">
								</div>
								<div class="col-lg-4">
									<img style="float: right;" src="/img/default.PNG">
								</div>
							</div>
							<div class="col-lg-12 btnDefault text-center blackWhite notSelected">
								<div class="col-lg-1">
									<i class="glyphicon glyphicon-adjust"></i>
								</div>
								<div class="col-lg-6 ">
									<p>Black & White</p>
								</div>
								<div class="col-lg-1">
								</div>
								<div class="col-lg-4">
									<img style="float: right;" src="/img/blackWhite.PNG">
								</div>
							</div>
						</div>
						<div class="col-lg-12 padding-top-20 padding-left-50 padding-right-50 addLocation33">
							<a class="btn btn-primary  btn-block addLocationBtn" role="button"><i class="fas fa-plus padding-right-20"></i>Save Design</a>
						</div>
						<div class="col-lg-6 padding-top-20" style="padding-left: 33px;">
							<a id="test" class="btn btn-primary  btn-block" role="button">Back</a>
						</div>
						<div class="col-lg-6 padding-top-20" style="padding-right: 33px;">
							<form id="formData" method="post" action="getData"> 
								<input style="display: none;" type="text" name="data">
								<a id="buttonData" class="btn btn-primary  btn-block" role="button">Next</a>
							</form>
						</div>
					</div>
					<!--end design slide-->
				</div>

				<!--start map slide-->
				<%include partials/basicMap%>
				<!--end map slide-->
			</div>
		</div>
	</body>
	<script type="text/javascript" src="/JS/locationsInput.js"></script>
	<script type="text/javascript" src="/JS/basicMap.js"></script>
	<!--<script type="text/javascript" src="/usersJS/101.js"></script>-->
	<script src=<%-JSON.stringify(gKey)%> async defer></script>
	<script>
		var key="<%=mkey%>";
	</script>
